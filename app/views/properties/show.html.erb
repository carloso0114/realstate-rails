<div>
  <p><%= link_to "Back", :back %></p>
</div>
<div>
  <p class="titulo">Public id</p>
  <p class="dato"><%= @property.public_id %></p>
  <p class="titulo">Title</p>
  <p class="dato"><%= @property.title %></p>
  <p class="titulo">Name</p>
  <p class="dato"><%= @property.property_type %></p>
  <p class="titulo">Location</p>
  <p class="dato"><%= @property.location.name %></p>
  <p class="titulo">Photo</p>
  <% if defined?(@property.property_images[0].url) %>
  <img src="<%= @property.property_images[0].url %>" alt="property pics"/>
  <% else %>
  <p>No photo available</p>
  <% end %>
</div>